{
    "nvidia": {
        "driver_version": "580.76.05",
        "runfile_url": "https://us.download.nvidia.com/XFree86/Linux-x86_64/580.76.05/NVIDIA-Linux-x86_64-580.76.05.run"
    },
    "network": {
        "nfs_server": "10.0.0.13",
        "default_subnet": "10.0.0.0/24"
    },
    "samba": {
        "user": "heads",
        "shares": [
            {
                "path": "quickOS/shared-prod-data",
                "options": "browseable=yes writable=yes"
            },
            {
                "path": "quickOS/shared-prod-data-sync",
                "options": "browseable=yes writable=yes"
            },
            {
                "path": "fastData/shared-test-data",
                "options": "browseable=yes writable=yes"
            },
            {
                "path": "fastData/shared-bulk-data",
                "options": "browseable=yes writable=yes"
            },
            {
                "path": "fastData/shared-test-data-sync",
                "options": "browseable=yes writable=yes"
            }
        ]
    },
    "zfs": {
        "pools": [
            {
                "name": "quickOS"
            },
            {
                "name": "fastData"
            }
        ],
        "datasets": [
            {
                "name": "quickOS/vm-disks",
                "properties": {
                    "recordsize": "128K",
                    "compression": "lz4",
                    "sync": "standard",
                    "quota": "800G"
                }
            },
            {
                "name": "quickOS/lxc-disks",
                "properties": {
                    "recordsize": "16K",
                    "compression": "lz4",
                    "sync": "standard",
                    "quota": "600G"
                }
            },
            {
                "name": "quickOS/shared-prod-data",
                "properties": {
                    "recordsize": "128K",
                    "compression": "lz4",
                    "sync": "standard",
                    "quota": "400G"
                }
            },
            {
                "name": "quickOS/shared-prod-data-sync",
                "properties": {
                    "recordsize": "16K",
                    "compression": "lz4",
                    "sync": "always",
                    "quota": "100G"
                }
            },
            {
                "name": "fastData/shared-test-data",
                "properties": {
                    "recordsize": "128K",
                    "compression": "lz4",
                    "sync": "standard",
                    "quota": "500G"
                }
            },
            {
                "name": "fastData/shared-backups",
                "properties": {
                    "recordsize": "1M",
                    "compression": "zstd",
                    "sync": "standard",
                    "quota": "2T"
                }
            },
            {
                "name": "fastData/shared-iso",
                "properties": {
                    "recordsize": "1M",
                    "compression": "lz4",
                    "sync": "standard",
                    "quota": "100G"
                }
            },
            {
                "name": "fastData/shared-bulk-data",
                "properties": {
                    "recordsize": "1M",
                    "compression": "lz4",
                    "sync": "standard",
                    "quota": "1.4T"
                }
            },
            {
                "name": "fastData/shared-test-data-sync",
                "properties": {
                    "recordsize": "16K",
                    "compression": "lz4",
                    "sync": "always",
                    "quota": "100G"
                }
            }
        ],
        "dataset_storage_types": [
            {
                "dataset": "quickOS/vm-disks",
                "storage_type": "zfspool:images"
            },
            {
                "dataset": "quickOS/lxc-disks",
                "storage_type": "zfspool:rootdir"
            },
            {
                "dataset": "quickOS/shared-prod-data",
                "storage_type": "cifs:images"
            },
            {
                "dataset": "quickOS/shared-prod-data-sync",
                "storage_type": "cifs:images"
            },
            {
                "dataset": "fastData/shared-backups",
                "storage_type": "dir:backup"
            },
            {
                "dataset": "fastData/shared-iso",
                "storage_type": "dir:iso,vztmpl"
            },
            {
                "dataset": "fastData/shared-test-data",
                "storage_type": "cifs:images"
            },
            {
                "dataset": "fastData/shared-bulk-data",
                "storage_type": "cifs:images"
            },
            {
                "dataset": "fastData/shared-test-data-sync",
                "storage_type": "cifs:images"
            }
        ],
        "arc_max": "32212254720"
    },
    "nfs": {
        "shares": [
            {
                "path": "quickOS/shared-prod-data",
                "options": "rw,async,no_subtree_check,noatime"
            },
            {
                "path": "quickOS/shared-prod-data-sync",
                "options": "rw,sync,no_subtree_check,noatime"
            },
            {
                "path": "fastData/shared-test-data",
                "options": "rw,async,no_subtree_check,noatime"
            },
            {
                "path": "fastData/shared-backups",
                "options": "rw,async,no_subtree_check,noatime"
            },
            {
                "path": "fastData/shared-iso",
                "options": "rw,async,no_subtree_check,noatime"
            },
            {
                "path": "fastData/shared-bulk-data",
                "options": "rw,async,no_subtree_check,noatime"
            },
            {
                "path": "fastData/shared-test-data-sync",
                "options": "rw,sync,no_subtree_check,noatime"
            }
        ]
    },
    "proxmox_storage_ids": {
        "quickos_vm": "quickOS-vm",
        "quickos_lxc": "quickOS-lxc",
        "fastdata_backup": "fastData-backup",
        "fastdata_iso": "fastData-iso"
    },
    "mount_point_base": "/mnt/pve"
}